#!/usr/bin/env bash
################################################################### SETUP ########################################################################
set -o errexit -o errtrace -o nounset
##################################################################################################################################################

function main() {
  #test "$(d container inspect -f '{{.State.Running}}' "$@" 2>/dev/null)" = "true"
  test "$(d_container_status "$@" 2>/dev/null)" = "running"
}

main "$@"
